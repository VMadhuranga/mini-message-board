<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <div>
      <% messages.forEach(message => { %>
        <div>
          <h3><%= message.user %></h3>
          <p><%= message.text %></p>
          <p><%= message.added.toLocaleString() %></p>
        </div>
      <% }) %>
    </div>
  </body>
</html>
